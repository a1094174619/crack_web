
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option txpower '18'
	option country 'US'
	option disabled '0'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'DialLink'
	option encryption 'none'

config wifi-iface
	option ssid 'CMCC-EDU'
	option encryption 'none'
	option device 'radio0'
	option mode 'sta'
	option network 'wwan'

